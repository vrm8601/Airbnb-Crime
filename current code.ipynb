    "ld=listings_detail[['price','transit','access','house_rules','property_type','room_type','accommodates','beds','bathrooms','bedrooms','instant_bookable','cancellation_policy']]\n",
    "cdmergeldls=pd.merge(cdmergels, ld, left_on='listing_id', right_index=True, how='left')\n"
   ]
